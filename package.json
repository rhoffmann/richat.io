{
  "name": "richat.io",
  "version": "0.0.1",
  "description": "the best chat app for rich people",
  "main": "./",
  "scripts": {
    "test": "node node_modules/jest-cli/bin/jest.js",
    "test-mocha": "mocha --reporter spec --compilers jsx:src/test/compiler.js src/test/test-helper.js src/test/*test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rhoffmann/richat.io.git"
  },
  "keywords": [
    "flux",
    "chat"
  ],
  "author": "Richard Hoffmann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rhoffmann/richat.io.git/issues"
  },
  "homepage": "https://github.com/rhofmann/richat.io.git",
  "devDependencies": {
    "babel": "^5.1.13",
    "babel-eslint": "^2.0.2",
    "babel-jest": "^5.0.1",
    "babel-runtime": "^5.1.13",
    "babelify": "^6.0.2",
    "bower": "^1.4.1",
    "browser-sync": "^2.5.3",
    "browserify": "^9.0.8",
    "browserify-shim": "^3.8.5",
    "gulp": "^3.8.11",
    "gulp-autoprefixer": "^2.1.0",
    "gulp-awspublish": "^1.0.6",
    "gulp-eslint": "^0.8.0",
    "gulp-if": "^1.2.5",
    "gulp-jshint": "^1.10.0",
    "gulp-load-plugins": "^0.10.0",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^1.3.3",
    "gulp-sourcemaps": "^1.5.1",
    "gulp-streamify": "0.0.5",
    "gulp-uglify": "^1.1.0",
    "gulp-util": "^3.0.4",
    "jest-cli": "^0.4.0",
    "jsdom": "^5.0.1",
    "jasmine-core": "^2.3.0",
    "karma-browserify": "^4.1.2",
    "karma-chrome-launcher": "^0.1.8",
    "karma-firefox-launcher": "^0.1.4",
    "karma-jasmine": "^0.3.5",
    "karma-phantomjs-launcher": "^0.1.4",
    "karma-safari-launcher": "^0.1.1",
    "karma-spec-reporter": "0.0.19",
    "phantomjs": "^1.9.16",
    "jshint-stylish": "^1.0.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.1.0"
  },
  "engines": {
    "node": "~0.12"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testDirectoryName": "tests",
    "testFileExtensions": [
      "js",
      "jsx"
    ],
    "testPathDirs": [
      "src/js/"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-tools"
    ]
  },
  "dependencies": {
    "firebase": "^2.2.4",
    "fireproof": "^2.5.2",
    "flux": "^2.0.1",
    "lodash": "^3.7.0",
    "q": "^1.3.0",
    "react": "^0.13.1",
    "react-tools": "^0.13.1"
  }
}
